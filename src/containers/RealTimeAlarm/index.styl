.real-time-alarm-wrapper {
  position fixed;
  right: 108px;
  top: 96px;
  bottom: 100px;
  overflow: hidden;
  pointer-events: none;
  img {
    pointer-events: auto;
    cursor pointer;
  }
  .real-time-alarm-info {
    // border: 1px solid red;
    width: 447px;
    display: flex;
    flex-direction: column;
    height: 100%;
    pointer-events: auto;
    .alarm-item-list {
      margin: 0;
      margin-top: -2px;
      padding: 0 10px;
      list-style: none;
      border: 2px solid #20AAFF66;
      flex: auto;
      height 100%;
      position relative;
      overflow-x hidden;
      overflow-y auto;
    }
  }
}
