.network-alarm-history-wrapper {
  position absolute;
  left: 108px;
  top: 96px;
  width: 506px;
  border: 1px solid #20AAFF;
  color: #fff;
  .type-tab {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin: 10px 0 8px;
    span {
      flex: auto;
      text-align: center;
      height: 38px;
      line-height: 38px;
      border: 2px solid #20AAFF;
      background: rgba(32,170,255,0.50);
      margin-right: 5px;
      &:last-child {
        margin-right: 0;
      }
      &.active {
        background: #20AAFF;
      }
    }
  }
}