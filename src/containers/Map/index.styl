.map-wrapper {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #00192e !important;
  .car-marker-popup {
    .leaflet-popup-content-wrapper {
      background: #20AAFF;
      border-radius: 0;
      color: #fff;
      font-size: 18px;
      .leaflet-popup-content {
        margin: 2px 8px;
      }
    }
    .leaflet-popup-tip-container {
      .leaflet-popup-tip {
        background: #20AAFF;
      }
    }
  }
}